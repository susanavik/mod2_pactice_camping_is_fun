<%= form_for @signup do |f| %>
    <%= f.label :name %>
    <%=f.collection_select :camper_id, @campers, :name, :age %>

    <%= f.label :activity %>
    <%=f.collection_select :activity_id, @activities, :name, :difficulty %>
    
    <%= f.label :time %>
    <%= f.number_field :time %>

    <%= f.submit :save %>
<% end %>